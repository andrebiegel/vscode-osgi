{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "OSGi",
	"patterns": [
		{
			"include": "#header-name"
		},
		{
			"include": "#instruction-name"
		}
	],
	"repository": {
		"header-name": {
			"name": "keyword.other.header-name.osgi",
			"patterns": [
				{
					"name": "keyword.other.header-name.osgi",
					"match": "^([A-Z]){1}([a-zA-Z]|-|_|\\d)*(:\\s)"
				}
			]
		},
		"instruction-name": {
			"name": "keyword.other.instruction-name.osgi",
			"patterns": [
				{
					"name": "keyword.other.instruction-name.osgi",
					"match": "^-([a-z]|.|-)*(:\\s)"
				}
			]
		}
	},
	"scopeName": "source.osgi"
}